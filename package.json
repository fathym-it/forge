	"scripts": {
		"deploy": "npm version patch && npm run deploy:all",
		"deploy:all": "npm run deploy:common && parallelshell \"npm run deploy:forge\" \"npm run deploy:marketing\" \"npm run deploy:no-ent\" \"npm run deploy:sign-in\" \"npm run deploy:register\" \"npm run deploy:lcu-display\" \"npm run deploy:lcu-builder\"",
		"deploy:common": "npm version patch --prefix projects/common && ng build common && npm run styles:common && npm publish ./dist/common --access public",
		"deploy:forge": "npm version patch --prefix projects/forge && lcu daf Switch --project=forge --env=prod && ng build forge --prod && npm publish ./dist/forge --access public",
		"deploy:lcu-builder": "npm version patch --prefix projects/lcu-builder && lcu daf Switch --project=lcu-builder --env=prod && ng build lcu-builder --prod && npm publish ./dist/lcu-builder --access public",
		"deploy:lcu-display": "npm version patch --prefix projects/lcu-display && lcu daf Switch --project=lcu-display --env=prod && ng build lcu-display --prod && npm publish ./dist/lcu-display --access public",
		"deploy:marketing": "npm version patch --prefix projects/marketing && lcu daf Switch --project=marketing --env=prod && ng build marketing --prod && npm publish ./dist/marketing --access public",
		"deploy:no-ent": "npm version patch --prefix projects/no-ent && lcu daf Switch --project=no-ent --env=prod && ng build no-ent --prod && npm publish ./dist/no-ent --access public",
		"deploy:register": "npm version patch --prefix projects/register && lcu daf Switch --project=register --env=prod && ng build register --prod && npm publish ./dist/register --access public",
		"deploy:sign-in": "npm version patch --prefix projects/sign-in && lcu daf Switch --project=sign-in --env=prod && ng build sign-in --prod && npm publish ./dist/sign-in --access public",
		"styles:common": "npm run styles:common:css && npm run styles:common:scss",
		"styles:common:css": "cpx \"./projects/common/src/styles/**/*.css\" \"./dist/common/styles/\"",
		"styles:common:scss": "cpx \"./projects/common/src/styles/**/*.scss\" \"./dist/common/styles/\"",
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e"
	},
